{
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "semi": false,
        "singleQuote": true
      }
    ],
    "prettier/semi": 0,
    "space-before-function-paren": 0,
    "comma-dangle": 0,
    "jsx-quotes": 0
  },
  "env": {
    "jest": true
  }
}